<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="anchor_pane" prefHeight="1080.0" prefWidth="1920.0" stylesheets="@../Shared/style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Login_Page.LoginPageController">
    <children>
        <Pane prefHeight="1080.0" prefWidth="1920.0" styleClass="overlay_box" />
        <ToolBar id="tool_bar" opacity="0.8" prefHeight="50.0" prefWidth="1920.0" AnchorPane.topAnchor="0.0">
            <items>
                <Button fx:id="loginButton" mnemonicParsing="false" text="Login" textFill="WHITE" />
            </items>
        </ToolBar>
        <VBox alignment="CENTER" fillWidth="false" prefHeight="1080.0" prefWidth="1920.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
            <VBox id="login_box" alignment="CENTER" fillWidth="false" prefHeight="500.0" prefWidth="500.0" styleClass="login_box" stylesheets="@../Shared/style.css">
               <children>
                  <TextField>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField />
               </children></VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
